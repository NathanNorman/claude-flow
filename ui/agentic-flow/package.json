{
  "name": "agentic-flow-ui",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "concurrently \"node server/index.ts\" \"node server/sync-manager.ts\"",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev\"",
    "dev:api": "node server.js",
    "setup": "npm install && echo 'Setup complete! Run npm run start to start all services.'",
    "build": "tsc && vite build",
    "build:server": "tsc server/index.ts --outDir dist",
    "build:full": "npm run build:server && npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "./start.sh",
    "stop": "./stop.sh",
    "start:api": "node dist/server.js",
    "start:full": "concurrently \"npm run start:api\" \"npm run preview\"",
    "test:mcp": "curl -X GET http://localhost:3001/api/health && curl -X GET http://localhost:3001/api/mcp/tools",
    "test:integration": "npm run test:mcp && curl -X GET http://localhost:3007/health"
  },
  "dependencies": {
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.12",
    "ansi-to-html": "^0.7.2",
    "chart.js": "^4.4.1",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.309.0",
    "node-fetch": "^3.3.2",
    "node-pty": "^1.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-flow-renderer": "^10.3.17",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.21.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.2.0",
    "three": "^0.160.0",
    "ws": "^8.14.2",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.9",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
